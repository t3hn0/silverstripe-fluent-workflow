{
    "name": "webbuilders-group/silverstripe-fluent-workflow",
    "description": "Adds compatibility to symbiote/silverstripe-advancedworkflow for tractorcow/silverstripe-fluent",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "advanced-workflow",
        "fluent"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Robert LeCreux",
            "homepage": "http://webbuildersgroup.com",
            "role": "Developer"
        }
    ],
    "require": {
        "silverstripe/framework": "*",
        "tractorcow/silverstripe-fluent": "*",
        "symbiote/silverstripe-advancedworkflow": "*"
    },
    "autoload": {
        "psr-4": {
            "WebbuildersGroup\\FluentWorkFlow\\": "src/"
        }
    },
    "support": {
        "issues": "https://github.com/webbuilders-group/silverstripe-fluent-workflow/issues"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}